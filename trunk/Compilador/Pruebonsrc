program Puta;
var
y:integer;
x:integer;
zorra: ^integer;
z:array [0..3] of integer;
p:array [0..3] of integer;
procedure par(var h: integer);
begin
write(h);
end;
begin
zorra^:=y;
zorra^:=z[0];
p:=z;
x:=0;
while (x<3) do
begin
z[x]:=x;
x:=x+1;
par(x);
end;
y:=89;
par(y);
x:=3;
y:=4;
end.
