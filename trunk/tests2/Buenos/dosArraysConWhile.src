program dosArraysConWhile;

var 
	x:integer;
	a: array[0..3] of integer;
	b: array[0..3] of integer;
begin
	x:=0;

	a[0]:=10;
	a[1]:=11;
	a[2]:=12;
	a[3]:=13;

	b[0]:=20;
	b[1]:=21;
	b[2]:=22;
	b[3]:=23;
	
	WHILE (x < 4) DO
	BEGIN
		write(x);
		write(a[x]);
		write(b[x]);
		x:=x+1;
	END;
end.